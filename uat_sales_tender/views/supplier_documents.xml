<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record model='ir.ui.view' id='supplier_tender_document_form_view'>
        <field name="name">supplier.tender.document.form</field>
        <field name="model">supplier.tender.document</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="doc_attachment_ids_supp" widget="many2many_binary" class="oe_inline"/>
                        </group>
                        <group>
                            <field name="date"/>
                            <field name="type"/>
                        </group>
                    </group>
                    <group>
                        <field name="description"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record model='ir.ui.view' id='supplier_tender_document_tree_view'>
        <field name="name">supplier.tender.document.tree</field>
        <field name="model">supplier.tender.document</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="date"/>
                <field name="type"/>
                <field name="description"/>
            </tree>
        </field>
    </record>

    <record id="supplier_document_action" model="ir.actions.act_window">
            <field name="name">Supplier Documents</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">supplier.tender.document</field>
            <field name="view_mode">tree,form</field>
        </record>


</odoo>