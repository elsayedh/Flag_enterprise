<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sales.tender -->
        <record id="seq_sales_tender" model="ir.sequence">
            <field name="name">Sales Tender</field>
            <field name="code">sales.tender</field>
            <field name="prefix">TN</field>
            <field name="padding">3</field>
        </record>

        <!-- Sequences for tender.document -->
        <record id="seq_tender_document" model="ir.sequence">
            <field name="name">Tender Documents</field>
            <field name="code">tender.document</field>
            <field name="prefix">DOC</field>
            <field name="padding">3</field>
        </record>


        <!-- Sequences for supplier.tender -->
        <record id="seq_supplier_tender" model="ir.sequence">
            <field name="name">Product Tender Documents</field>
            <field name="code">supplier.tender</field>
            <field name="prefix">SUP</field>
            <field name="padding">3</field>
        </record>

        <!-- Sequences for supplier.tender.document -->
        <record id="seq_supplier_tender_documents" model="ir.sequence">
            <field name="name">Tender Supplier Documents</field>
            <field name="code">supplier.tender.document</field>
            <field name="prefix">DOC</field>
            <field name="padding">3</field>
        </record>


    </data>
</odoo>
